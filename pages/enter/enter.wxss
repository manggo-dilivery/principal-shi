/* pages/join_info/join_info.wxss */
.list{
  width:92%;
  height:auto;
  margin:0 auto;
}
.item{
  display: flex;
  align-items: center;
  padding: 30rpx 0;
  border-bottom:1rpx solid #ddd;
}
.item text{
  width:150rpx;
  margin-left:10rpx;
  font-weight: bold;
  letter-spacing: 2rpx;
  color:#555;
}
.item-time{
  display: flex;
  align-items: center;
  padding: 20rpx 0;
  border-bottom:1rpx solid #ddd;
}
.item-time text{
  margin-left:10rpx;
  font-weight: bold;
  color:#555;
}
.item-time input{
  margin-left:20rpx;
}

.btn{
  width:100%;
  color:#fff;
  background: #ff9f40;
  margin-top:60rpx;
}
.btn:hover{
  background: #ee8e30;
  margin-top:60rpx;
}
/*已有账号  */
.no-account{
  float:right;
  margin-top:20rpx;
}

/*底部tabbar样式  */

.bottom_tabbar{
  width:100%;
  height:90rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
  background: #fff;
  position: fixed;
  bottom:0;
  left:0;
  border-top:2rpx solid #eee;
}
.bottom_tabbar view{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}
.bottom_tabbar image{
  width:45rpx;
  height:45rpx;
}
.business-enter{
  width:47rpx;
  height:47rpx;
}
.bottom_tabbar_plus{
  position:absolute;
  width:75rpx !important;
  height:75rpx !important;
  top:-30rpx;
}
.active{
  color:#fe9e4a;
}
/*验证码  */
.test{
  width:160rpx;
  font-size:30rpx;
  color:#555;
  border:1rpx solid #eee;
  padding: 10rpx 20rpx;
  border-radius:10rpx;
  box-shadow: 0 0 6rpx #eee;
}
.test:hover{
  background: #eee;
}
